{
    "files.insertFinalNewline": true,
    "rust-analyzer.cargo.features": ["full"],
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true
    },
    "[shellscript]": {
      "editor.formatOnSave": true,
      "editor.codeActionsOnSave": {
        "source.fixAll.shellcheck": true
      }      
    },
}
